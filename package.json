{
  "name": "ghamiz",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "start": "rm -rf ./src/styles/styles.css && npm run twild &&  npm run dev",
    "build": "tsc && vite build && npx tailwindcss -o ./dist/styles.css --minify",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "twbuild": "npx tailwindcss -i ./src/styles/index.css -o ./src/styles/styles.css --watch",
    "twild": "npx tailwindcss build -o ./src/styles/styles.css"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.5",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "mini-svg-data-uri": "^1.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  }
}
